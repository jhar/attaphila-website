doctype html
html(lang="en")
	head
		title= title
	body
		each message in messages
			div(class="flash")= message
		form(action="/signup", method="post")
			div
				label User Name:
				input(type="text", name="username")
			div
				label Password:
				input(type="password", name="password")
			div
				input(type="submit", value="Sign Up")